{
  "defwidget@terminal": null,
  "defwidget@files": null,
  "defwidget@web": null,
  "defwidget@ai": null,
  "defwidget@sysinfo": null,
  "defwidget@docs": null,
  "yoyo@claude": {
    "icon": "terminal",
    "label": "Yoyo-Cli",
    "color": "#00BCD4",
    "description": "Toggle yoyo-cli terminal pane",
    "blockdef": {
      "meta": {
        "view": "term",
        "controller": "shell",
        "cmd:initscript": "export PATH=\"${WAVETERM_WSHBINDIR:-$HOME/.local/share/waveterm/bin}:$PATH\"; TOGGLE=\"$HOME/.yoyo-dev/setup/wave-toggle.sh\"; bash \"$TOGGLE\" yoyo-cli --self-delete"
      }
    }
  },
  "yoyo@files": {
    "icon": "folder-open",
    "label": "Files",
    "color": "#E85D04",
    "description": "Toggle project file browser pane",
    "blockdef": {
      "meta": {
        "view": "term",
        "controller": "shell",
        "cmd:initscript": "export PATH=\"${WAVETERM_WSHBINDIR:-$HOME/.local/share/waveterm/bin}:$PATH\"; TOGGLE=\"$HOME/.yoyo-dev/setup/wave-toggle.sh\"; bash \"$TOGGLE\" files --self-delete"
      }
    }
  },
  "yoyo@gui": {
    "icon": "gauge",
    "label": "GUI",
    "color": "#E85D04",
    "description": "Toggle Yoyo Dev Dashboard pane",
    "blockdef": {
      "meta": {
        "view": "term",
        "controller": "shell",
        "cmd:initscript": "export PATH=\"${WAVETERM_WSHBINDIR:-$HOME/.local/share/waveterm/bin}:$PATH\"; TOGGLE=\"$HOME/.yoyo-dev/setup/wave-toggle.sh\"; bash \"$TOGGLE\" gui --self-delete"
      }
    }
  },
  "yoyo@terminal": {
    "icon": "rectangle-terminal",
    "label": "Terminal",
    "color": "#E85D04",
    "description": "Toggle a bash terminal pane in the project directory",
    "blockdef": {
      "meta": {
        "view": "term",
        "controller": "shell",
        "cmd:initscript": "export PATH=\"${WAVETERM_WSHBINDIR:-$HOME/.local/share/waveterm/bin}:$PATH\"; TOGGLE=\"$HOME/.yoyo-dev/setup/wave-toggle.sh\"; bash \"$TOGGLE\" terminal --self-delete"
      }
    }
  }
}
