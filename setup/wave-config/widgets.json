{
  "defwidget@terminal": null,
  "defwidget@files": null,
  "defwidget@web": null,
  "defwidget@ai": null,
  "defwidget@sysinfo": null,
  "defwidget@docs": null,
  "yoyo@claude": {
    "icon": "terminal",
    "label": "Yoyo-Cli",
    "color": "#E85D04",
    "description": "Launch yoyo-dev-ai framework with Claude Code in terminal",
    "blockdef": {
      "meta": {
        "view": "term",
        "controller": "shell",
        "cmd:initscript": "cd \"${YOYO_PROJECT_DIR:-$PWD}\" && (command -v yoyo-cli >/dev/null && yoyo-cli || claude)"
      }
    }
  },
  "yoyo@files": {
    "icon": "folder-open",
    "label": "Files",
    "color": "#E85D04",
    "description": "Project file browser",
    "blockdef": {
      "meta": {
        "view": "preview",
        "file": "${YOYO_PROJECT_DIR:-~}"
      }
    }
  },
  "yoyo@gui": {
    "icon": "gauge",
    "label": "GUI",
    "color": "#E85D04",
    "description": "Yoyo Dev Dashboard",
    "blockdef": {
      "meta": {
        "view": "web",
        "url": "http://localhost:5173",
        "pinnedurl": "http://localhost:5173"
      }
    }
  },
  "yoyo@sysinfo": {
    "icon": "chart-line",
    "label": "System",
    "color": "#E85D04",
    "description": "System resource monitor",
    "blockdef": {
      "meta": {
        "view": "sysinfo",
        "sysinfo:type": "CPU + Mem"
      }
    }
  }
}
