#!/usr/bin/env node
/**
 * Yoyo AI CLI Launcher
 *
 * This script launches the Yoyo AI CLI.
 * It detects the best available runtime (Bun or Node.js).
 */

import { fileURLToPath } from 'url';
import { dirname, join } from 'path';

const __dirname = dirname(fileURLToPath(import.meta.url));

// Import and run the CLI
import(join(__dirname, '..', 'dist', 'cli', 'index.js'));
