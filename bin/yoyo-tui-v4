#!/usr/bin/env node
/**
 * TUI v4 Launcher Script
 *
 * Direct launcher for TUI v4 without going through main CLI.
 * This bypasses TypeScript build issues for now.
 */

import { fileURLToPath } from 'url';
import { dirname, join } from 'path';

const __dirname = dirname(fileURLToPath(import.meta.url));

// Launch TUI v4 directly with tsx
import { launchTUI } from '../src/tui-v4/index.js';

launchTUI().catch((error) => {
  console.error('Failed to launch TUI:', error);
  process.exit(1);
});